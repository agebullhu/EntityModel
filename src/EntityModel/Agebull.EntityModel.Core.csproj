    <None Remove="DEncrypt\**" />
    <None Remove="Redis\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="Base\TableSql.cs" />
    <Compile Remove="Entity\ApiArrayResult.cs" />
    <Compile Remove="Entity\ApiFileResult.cs" />
    <Compile Remove="Entity\ApiValueResult.cs" />
    <Compile Remove="Helper\EditScope.cs" />
    <Compile Remove="Permissions\IPowerChecker.cs" />
    <Compile Remove="Permissions\IRolePower.cs" />
    <Compile Remove="Properties\AssemblyInfo.cs" />
    <Compile Remove="Status\EditArrestMode.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Agebull.Common.DataEntity.csproj.DotSettings" />
    <None Remove="Agebull.Common.DataEntity.csproj.vspscc" />
    <None Remove="app.config" />
