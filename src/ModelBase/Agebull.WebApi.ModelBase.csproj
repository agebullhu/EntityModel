    <None Remove="DataAccess\**" />
    <None Remove="docs\**" />
    <None Remove="Interface\**" />
    <None Remove="Ioc\**" />
    <None Remove="Page\**" />
    <None Remove="Type\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="BusinessLogic\BusinessLogicByHistory.cs" />
